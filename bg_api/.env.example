# OpenRoute API 配置 (必需)
OPENROUTE_API_KEY=your_openroute_api_key_here

# PocketBase 配置 (必需)
POCKETBASE_URL=http://127.0.0.1:8090

# PocketBase 集合配置 (可选，默认 shouban)
POCKETBASE_COLLECTION=shouban

# 日志级别配置 (可选，默认 INFO)
# 可选值: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# 服务器配置 (可选)
HOST=0.0.0.0
PORT=8000
DEBUG=True